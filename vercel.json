{
    "rewrites": [
      {
        "source": "/:locale/docs/:path*",
        "destination": "/:locale/index.html"
      },
      {
        "source": "/:locale/blog/:path*",
        "destination": "/:locale/index.html"
      },
      {
        "source": "/:locale/(.*)",
        "destination": "/:locale/index.html"
      },
      {
        "source": "/docs/:path*",
        "destination": "/index.html"
      },
      {
        "source": "/blog/:path*",
        "destination": "/index.html"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }